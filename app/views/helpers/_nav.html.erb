<div class="header-container">
  <header>
    <div class="logo"><%= link_to 'science fair', root_path %></div>
    <nav>
      <ul>
        <li class="nav-item"><%= link_to 'About', about_path %></li>
        <% if current_user %>
          <li class="nav-item"><%= link_to 'Log Out', logout_path, method: :delete %></li>
        <% else %>
          <li class="nav-item"><%= link_to 'Log In', login_path %></li>
          <li class="nav-item"><%= link_to 'Sign Up', new_user_path %></li>
        <% end %>
      </ul>
    </nav>
  </header>
</div>
