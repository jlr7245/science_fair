<%= form_for [@cohort, @project] do |f| %>
  <div>
    <%= f.label :name, class: "for_text_inputs" %>
    <%= f.text_field :name, autofocus: true %>
  </div>

  <div>
    <%= f.label :due_date, class: "for_text_inputs" %>
    <div class="dates"> <%= f.date_select :due_date %></div>
   
  </div>

    <div>
    <%= f.submit :submit %>
  </div>
<% end %>